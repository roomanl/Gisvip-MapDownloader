{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Gisvip地图下载器",
  "version": "0.1.0",
  "identifier": "cn.gisvip.map",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Gisvip地图下载器",
        "label": "main",
        "width": 1100,
        "height": 700,
        "minWidth": 1100,
        "minHeight": 700,
        "center": true,
        "decorations": false,
        "visible": false
      },{
        "title": "瓦片预览",
        "label": "previewTiles",
        "url": "previewTiles",
        "width": 1100,
        "height": 700,
        "minWidth": 1100,
        "minHeight": 700,
        "center": true,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "nsis"
    ],
    "resources": [],
    "windows": {
      "wix": {
        "language": "zh-CN"
      },
      "nsis": {
        "languages": [
          "SimpChinese"
        ],
        "installerIcon": "icons/icon.ico"
      },
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}